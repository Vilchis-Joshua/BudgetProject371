<resources>

    <!-- Base application theme. -->
    <!-- Changing to .NoActionBar got rid of the banner at the top-->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#49796b</item>
        <item name="colorPrimaryDark">#175e08</item>
        <item name="colorAccent">#317873</item>
    </style>

    <style name="MyTitleStyle" parents="@android:style/Widget.TextView">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">sans-serif-smallcaps</item>
    </style>

    <style name="BudgetTree"></style>

</resources>


